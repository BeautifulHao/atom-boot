<mapper namespace="com.atom.smart.sys.mapper.UserFunctionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.atom.smart.sys.entity.UserFunction">
        <id column="id" property="id" />
